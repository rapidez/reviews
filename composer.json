{
    "name": "rapidez/reviews",
    "description": "Rapidez Reviews",
    "keywords": [
        "rapidez"
    ],
    "homepage": "https://rapidez.io",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Claudio Ferraro",
            "email": "claudio@justbetter.nl",
            "homepage": "https://justbetter.nl",
            "role": "Developer at JustBetter"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/view": "^9.0|^10.0|^11.0",
        "rapidez/core": "~0.57|^1.0|^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Rapidez\\Reviews\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rapidez\\Reviews\\ReviewsServiceProvider"
            ]
        }
    }
}
